---
import AccordionItem from './AccordionItem.astro';

const content = [
  { title: 'First Item', description: 'Toma por curioso' },
  { title: 'Second Item', description: 'Mira que te como ehh > El dibu' },
  { title: 'Third Item', description: 'La curiosidad mato al gato. - Julito' },
];
---

{
  content.map((item, i) => (
    <AccordionItem
      index={i + 1}
      title={item.title}
      description={item.description}
    />
  ))
}
